{
  "name": "webstrates-editor",
  "displayName": "Webstrates Editor",
  "description": "Webstrates Editor",
  "version": "0.0.1",
  "publisher": "raedle",
  "engines": {
    "vscode": "^1.4.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "workspaceContains:.webstrates/config.json",
    "onCommand:webstrates.initWorkspace"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "commands": [
      {
        "command": "webstrates.initWorkspace",
        "title": "Init Webstrates Workspace"
      },
      {
        "command": "webstrates.openWebstrate",
        "title": "Open Webstrate"
      },
      {
        "command": "webstrates.showWebstratePreview",
        "title": "Show Webstrate Preview"
      }
    ],
    "keybindings": [
      {
        "command": "webstrates.openWebstrate",
        "key": "ctrl+o",
        "mac": "cmd+o"
      }
    ],
    "languages": [
      {
        "id": "html",
        "firstLine": ".*<html.*>.*"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "node ./node_modules/vscode/bin/compile",
    "compile": "node ./node_modules/vscode/bin/compile -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "dependencies": {
    "chokidar": "^1.6.0",
    "json-comments": "^0.2.1",
    "json0-ot-diff": "^1.0.1",
    "jsonml-parse": "^1.0.0",
    "jsonml-tools": "git+http://github.com/cklokmose/jsonml-tools.git#master",
    "node-json-minify": "^1.0.0",
    "optimist": "^0.6.1",
    "sharedb": "^0.11.37",
    "websocket": "^1.0.23"
  },
  "devDependencies": {
    "typescript": "^1.8.5",
    "vscode": "^0.11.0"
  }
}
